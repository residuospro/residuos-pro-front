		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"postcss": "^8.4.24",
		"prettier": "^2.4.1",
		"tailwindcss": "^3.3.2",
		"typescript": "~4.5.5"
	},
	"lint-staged": {
		"**/*.{js,ts,vue}": [
			"eslint --fix",
			"prettier --write"
