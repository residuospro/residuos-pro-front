		"core-js": "^3.8.3",
		"mdi-vue": "^3.0.13",
		"pinia": "^2.1.3",
		"vue": "^3.2.13",
		"vue-router": "^4.0.3",
		"vue3-spinners": "^1.2.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.4.2",
		"@commitlint/config-conventional": "^17.4.2",
		"@types/node": "^20.2.5",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"@vue/cli-plugin-babel": "~5.0.0",
